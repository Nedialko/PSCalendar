    <command:details>
      <command:name>Get-Calendar</command:name>
      <command:verb>Get</command:verb>
      <command:noun>Calendar</command:noun>
      <maml:description>
        <maml:para>Displays a visual representation of a calendar.</maml:para>
      </maml:description>
    </command:details>
    <maml:description>
      <maml:para>This command displays a visual representation of a calendar. It supports multiple months, as well as the ability to highlight a specific date or dates.</maml:para>
    </maml:description>
